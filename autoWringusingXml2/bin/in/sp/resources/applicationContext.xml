<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
     <bean class = "in.sp.beans.Address" id = "address">
     	<property name = "houseNo" value = "123"/>
     	<property name = "city" value = "Latur"/>
     	<property name = "pincode" value = "413512"/>
     </bean>
    <bean class = "in.sp.beans.Subjects" id = "subjects">
     <property name = "subjects" >
     <list>
     	<value>c</value>
     	<value>c++</value>
     	<value>Java</value>
     </list>
     </property>
    
   
    
    </bean>
    
    
    
    <bean class = "in.sp.beans.Student" id = "stdId" autowire = "byName" >
     	<property name = "name" value = "Atharav"/>
     	<property name = "rollNo" value = "121"/> 
     	
     </bean>

</beans>